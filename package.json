{
  "name": "ain-adk-providers",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "packages/memory/*",
    "packages/models/*",
    "packages/auth/*"
  ],
  "scripts": {
    "build": "lerna run build",
    "clean": "lerna run clean && lerna clean",
    "publish:lerna": "lerna run build && lerna publish",
    "lint": "npx @biomejs/biome check",
    "lint:write": "npx @biomejs/biome check --write",
    "update:adk": "lerna exec --concurrency 1 -- yarn add @ainetwork/adk"
  },
  "devDependencies": {
    "lerna": "^8.2.3",
    "tsup": "^8.2.3",
    "typescript": "^5.0.0"
  },
  "license": "MIT"
}
